{
  "Enabled": true,
  "RelayGameToDiscord": true,
  "RelayDiscordToGame": true,
  "Discord": {
    "BotToken": "",
    "ChannelId": "",
    "WebhookUrl": "",
    "IgnoreBotMessages": true,
    "IgnoreWebhookMessages": true,
    "AllowMentions": true
  },
  "Events": {
    "ServerStart": {
      "Enable": true,
      "Embed": {
        "Enable": true,
        "Color": "#008000"
      },
      "Message": "Server has started"
    },
    "ServerStop": {
      "Enable": true,
      "Embed": {
        "Enable": true,
        "Color": "#800000"
      },
      "Message": "Server has stopped"
    },
    "PlayerJoin": {
      "Enable": true,
      "Embed": {
        "Enable": true,
        "Color": "#00FF00"
      },
      "Message": "**%player% joined the game**"
    },
    "PlayerLeave": {
      "Enable": true,
      "Embed": {
        "Enable": true,
        "Color": "#FF0000"
      },
      "Message": "**%player% left the game**"
    },
    "WorldEnter": {
      "Enable": false,
      "Embed": {
        "Enable": false,
        "Color": "#5865F2"
      },
      "Message": "%player% entered %world%"
    },
    "WorldLeave": {
      "Enable": false,
      "Embed": {
        "Enable": false,
        "Color": "#5865F2"
      },
      "Message": "%player% left %world%"
    },
    "PlayerDeath": {
      "Enable": true,
      "Embed": {
        "Enable": false,
        "Color": "#FFA500"
      },
      "Message": "**%player% died**"
    },
    "PlayerKill": {
      "Enable": true,
      "Embed": {
        "Enable": false,
        "Color": "#FF0000"
      },
      "Message": "**%killer% eliminated %victim%**"
    }
  }
}